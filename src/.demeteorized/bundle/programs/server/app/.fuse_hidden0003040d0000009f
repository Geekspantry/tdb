            return Technologies.find({                                                                                 // 13
              _id: { $in: collection.technologiesId }                                                                  // 14
            });                                                                                                        //
          }                                                                                                            //
        }                                                                                                              //
                                                                                                                       //
        return find;                                                                                                   //
      }()                                                                                                              //
    }]                                                                                                                 //
  };                                                                                                                   //
});                                                                                                                    //
